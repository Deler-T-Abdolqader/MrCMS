@using MrCMS.Web.Admin.Models
@model MrCMS.Web.Admin.Models.UserStats
<div class="card">
<div class="card-body">
        <h4>@Html.Resource("Admin Dashboard User Stats", "User stats")</h4>
        <table class="table table-striped table-condensed" style="overflow: scroll">
            <thead>
                <tr>
                    <th>@Html.Resource("Active")</th>
                    <th>@Html.Resource("Inactive")</th>
                </tr>
            </thead>
            <tbody>
                <tr>
                    <td>@Model.ActiveUsers</td>
                    <td>@Model.InactiveUsers</td>
                </tr>
            </tbody>
        </table>
</div>
</div>
